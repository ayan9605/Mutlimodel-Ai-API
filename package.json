{
  "name": "openrouter-ai-api",
  "version": "1.0.0",
  "description": "High-performance AI API with intelligent model routing and auto-selection",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "prod": "NODE_ENV=production node --max-old-space-size=512 --optimize-for-size server.js",
    "cluster": "NODE_ENV=production node --max-old-space-size=512 --optimize-for-size --gc-interval=100 server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:monitor": "pm2 monit"
  },
  "keywords": [
    "ai",
    "api",
    "openrouter",
    "llm",
    "fastify",
    "high-performance",
    "streaming"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "fastify": "^4.28.1",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4"
  },
  "optionalDependencies": {
    "pm2": "^5.4.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
